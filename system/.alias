# After adding an aliase do: source ~/.bashrc

# Go to dir
alias cdproject='cd ~/Projects'
alias cdintra-spa='cd ~/Projects/intranet-spa'
alias cdf-app='cd ~/Projects/freask-app'
alias cdf-vir='cd ~/Projects/freask-virtuals'
alias cdp-id='cd ~/Projects/freask-app/plugins/Interdeposit'
alias cdaction='cd ~/Projects/actions'
alias cdmail='cd ~/Projects/mailing'
alias cdapi-id='cd ~/Projects/interdeposit-api'

alias cdapi-doc='cd ~/Projects/api-documentation'

# Serve local server
alias start_intra-spa='cd ~/Projects/intranet-spa && npm start'

# Java APP
alias bfg='java -jar ~/bfg-1.14.0.jar'

# SSH
alias sshdw='ssh dev-web1'
alias sshpw='ssh prod-web1'
alias sshdf='ssh dev-front'
alias sshppf='ssh pprod-front'

alias sshmysqldw='ssh -N -L 3306:localhost:3306 dev-web1'

alias sshpi='ssh home-pi'

# Perso
alias cdperso='cd ~/perso'
alias cdmc-kh-bot='cd ~/perso/mychastity.kh-bot-chaster'
alias cdmc-app='cd ~/perso/mychastity.app'
alias cdmc-api='~/perso/mychastity.api'
alias cdmc-chaster='cd ~/perso/mychastity.chaster-app-php'
alias cdauth-chaster='cd ~/perso/mychastity.oauth2-chaster-app'
alias cdmc-finage='cd ~/perso/mychastity.finage-php'
alias cdbeck='cd ~/Projects/beck-columns'

alias cddotfile='cd ~/.dotfiles'

# Aertec
alias cdaertec='cd ~/Projects/Aertec'
alias cdmagec='cd ~/Projects/Aertec/magec'

# Others
alias tabula='java -jar ~/tabula.jar'
alias pdfboxtext='java -jar ~/pdfbox.jar ExtractText'
alias pdfboximg='java -jar ~/pdfbox.jar ExtractImages'

# Sytem
alias showip="ip addr show eth0 | grep -oP '(?<=inet\s)\d+(\.\d+){3}'"

# Applications
# Dirty using MySQL
alias mysql_start='sudo /usr/local/mysqld/bin/mysqld -u root &' # dirty start
alias mysql_stop="sudo kill $(ps aux | grep '/usr/local/mysqld/bin/mysqld -u root' | awk '{print $2}')"
alias mysql='/usr/local/mysqld/bin/mysql'

alias mysqldw='mysql -u bh -p -h 127.0.0.1 -P 3306'
